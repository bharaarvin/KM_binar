<html>

<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    <script src="https://unpkg.com/feather-icons"></script>

    <style>
        .dark{
            color: black;
        }

        .carda{
            height: auto;
            width: auto;
            border-width: 3px;
            padding: 20px;
        }

        .feather{
            height: 40px;
            width: 40px;
        }

        .text{
            font-size: 25px;
            font-style:
        }
    </style>
</head>

<body>
    <section id="nav">
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
            <div class="container-fluid">
                <a class="navbar-brand btn bg-secondary p-3" href="#"></a>
              </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                        <li class="nav-item" style="padding-left: 25px; padding-top: 10px;">
                            <a class="nav-link active btn bg-secondary p-3 px-5" href="#"></a>
                        </li>
                        <li class="nav-item" style="padding-left: 20px; padding-top: 12px;">
                            <span class="navbar-toggler-icon bg-secondary"></span>
                        </li>
                    <form class="d-flex" style="padding-left: 20cm; padding-top: 3px;">
                        <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                        <button class="btn btn-outline-success" type="submit">Search</button>
                    </form>
                    <li class="nav-item dropdown" style="padding-left: 100px;">
                        <a class="nav-link dropdown-toggle dark" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown"
                            aria-expanded="false">
                            Bhara
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <li><a class="dropdown-item" href="#">Profile</a></li>
                            <li><a class="dropdown-item" href="#">Log Out</a></li>
                        </ul>
                    </li>
                    </ul>
                </div>
            </div>
        </nav>
    </section>
    <section id="content">
        <div class="row">
            <div class="col-md-2">
                <div class="row">
                    <div class="col-md-3">
                        <div>
                            <i style="margin-left: 10px; margin-top: 10px;" data-feather="home"></i>
                        </div>
                        <div>
                            <i style="margin-left: 10px; margin-top: 15px;" data-feather="shopping-cart"></i>
                        </div>
                    </div>
                    <div class="col-md-9">
                        <div class="container">
                            <p class="text fw-bold" style=" margin-top: 10px;">CARS</p>
                            <p class="text fw-bold" style=" margin-top: 15px;">List Car</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-10" style="background-color: #F8F8FF">
                <div class="container-fluid">
                    <div class="row">
                        <p class="text fw-bold" style=" margin-top: 15px;">List Car</p>
                        <button><a href="/cars/add" type="button" class="btn bg-primary">Add cars</a></button>
                        <% data.forEach(result => { %>
                            <div class="col-md-4">
                                <div class="card carda" style="margin-top: 30px;">
                                    <div class="content">
                                        <p>
                                            <%= result.nama %>
                                        </p>
                                        <p>
                                            <%= result.sewa %>
                                        </p>
                                        <p>
                                            <%= result.ukuran %>
                                        </p>
                                        <p>
                                            <img src="http:localhost:8082/static<%= result.foto %>" width="150" height="150" />
                                        </p>
                                    </div>
                                    <div>
                                        <a type="button" class="btn bg-success" href="/cars/update/<%= result.id %>">Edit</a>
                                        <a onclick="alert('Are You sure?')" class="btn bg-danger" href="/cars/<%= result.id %>">Delete</a>
                                    </div>
                                </div>
                            </div>
                            <% }) %>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <script>
        feather.replace()
    </script>
</body>

</html>